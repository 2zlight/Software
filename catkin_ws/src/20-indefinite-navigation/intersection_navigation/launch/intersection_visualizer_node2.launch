<launch>
	<arg name="robot_name" default="daisy" />

    <remap from="~img" to="intersection_localizer_node/localizer_debug_out" />

	<node ns="$(arg robot_name)" name="intersection_visualizer_node2" pkg="intersection_navigation" type="intersection_visualizer2.py" output="screen">
		<!-- Setup parameters -->
		<param name="~robot_name" value="$(arg robot_name)"/>
	</node>
</launch>
