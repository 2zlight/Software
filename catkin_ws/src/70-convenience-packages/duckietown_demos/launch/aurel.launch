<launch>
	<arg name="veh" default="$(env VEHICLE_NAME)"/>
	<!-- start basic args -->
	<include file="$(find duckietown_demos)/launch/master.launch">
		<arg name="veh" value="$(arg veh)"/>
		<arg name="visualization" value="true" />
		
		<arg name="/camera/raw" value="true"/>
		<arg name="/camera/raw/rect" value="true"/>
		<arg name="verbose" default="true" />
		<arg name="param_file_name" value="default" />
		<arg name="line_detector_param_file_name" value="default" />
		<arg name="anti_instagram" value="true" />
		<arg name="lane_following" value="true"/>
		<arg name="/lane_following/line_detection" default="true"/>
		<arg name="wheels" default="false"/>
		<arg name="joystick" default="false"/>
		<arg name="/lane_following/lane_filter" default="false"/>
		<arg name="/lane_following/ground_projection" default="false"/>
		<arg name="fsm" default="false"/>
		<arg name="LED" default="false" />
		<arg name="vehicle_avoidance" default="false"/>
	</include>
</launch>
