<launch>
    <arg name="veh" doc="Name of vehicle. ex: megaman"/>
    <arg name="map_name" default="duckietown_map" />
    <arg name="publish_solution" default="True" />
	<param name="map_name" value="$(arg map_name)" />
	<param name="publish_solution" value="$(arg publish_solution)" />
    <param name="veh" value="$(arg veh)"/>

	<node
                ns="$(arg veh)"
		name="graph_search_server"
		pkg="navigation"
		type="graph_search_server.py"
		output="screen"
	/>

    <node
                ns="$(arg veh)"
		name="actions_dispatcher"
		pkg="navigation"
		type="actions_dispatcher.py"
		output="screen"
	/>

</launch>
